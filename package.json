    "stylelint": "^16.12.0",
    "stylelint-config-standard": "^36.0.1",
    "ts-loader": "^9.5.1",
    "typescript": "5.7.2",
    "typescript-eslint": "^8.19.1",
    "webpack": "^5.99.7"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
